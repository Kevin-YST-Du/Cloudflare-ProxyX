{
  "name": "proxyx",
  "version": "4.9.0",
  "description": "Ultimate Docker & Linux Recursive Proxy Server with GeoIP & SQLite Persistence",
  "main": "src/server.js",
  "bin": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "build-bin": "pkg . --out-path dist/ --compress GZip"
  },
  "pkg": {
    "scripts": [
      "src/**/*.js"
    ],
    "assets": [
      "node_modules/**/*",
      "node_modules/geoip-lite/data/**/*",
      "node_modules/better-sqlite3/build/Release/better_sqlite3.node"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-linux-arm64",
      "node18-win-x64",
      "node18-macos-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "dotenv": "^16.3.1",
    "geoip-lite": "^1.4.10",
    "better-sqlite3": "^9.4.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
